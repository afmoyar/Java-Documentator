@startuml
Interface Subject{
	+register()
	+unregister()
	+notifyObservers()
	+getUpdate()
}
Interface Observer{
	+update()
	+setSubject()
}
class MyTopic{
	-observers: List<Observer>
	-message: String
	-changed: boolean
	-MUTEX: Object
	+register()
	+unregister()
	+notifyObservers()
	+getUpdate()
	+postMessage()
}
MyTopic --> Observer
MyTopic .up.|> Subject
@enduml
